{
  "dashboard": {
    "title": "RAG System Metrics",
    "panels": [
      {
        "title": "Query Rate",
        "targets": [
          {
            "expr": "rate(rag_queries_total[1m])"
          }
        ]
      },
      {
        "title": "Query Duration (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rag_query_duration_seconds)"
          }
        ]
      },
      {
        "title": "Active Requests",
        "targets": [
          {
            "expr": "rag_active_requests"
          }
        ]
      }
    ]
  }
}
